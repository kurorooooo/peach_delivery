<p id="notice"><%= notice %></p>

<h1>Peaches</h1>

<table>
  <thead>
    <tr>
      <th>Restaurant</th>
      <th>Delivery time</th>
      <th>Required price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @peaches.each do |peach| %>
      <tr>
        <td><%= peach.restaurant %></td>
        <td><%= peach.delivery_time %></td>
        <td><%= peach.required_price %></td>
        <td><%= link_to 'Show', peach %></td>
        <td><%= link_to 'Edit', edit_peach_path(peach) %></td>
        <td><%= link_to 'Destroy', peach, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Peach', new_peach_path %>
